package com.awt;

import java.awt.Frame;

public class TestWindow extends Frame{

		public TestWindow() {
			System.out.println("Start...");
			
			this.setSize(1000, 1000);			
			this.setVisible(true);
		}
		
	public static void main(String[] args) {
		
		TestWindow testWindow = new TestWindow();

	}

}
