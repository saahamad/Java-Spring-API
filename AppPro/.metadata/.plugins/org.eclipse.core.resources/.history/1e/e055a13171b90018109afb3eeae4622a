package com.awt;

import java.awt.Button;
import java.awt.Frame;

public class TestFrame extends Frame{
	
		public TestFrame() {
			System.out.println("start..");
			
			Button b = new Button("Click me!");
			add(b);
			setSize(500, 500);
			setVisible(true);
		}

		public static void main(String[] args) {
			new TestFrame();
		}
}


